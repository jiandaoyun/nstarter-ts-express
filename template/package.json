    "cookie-parser": "1.4.6",
    "cron": "2.2.0",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "helmet": "6.0.1",
    "http-status": "1.6.2",
    "i18next": "22.4.9",
    "i18next-conv": "12.1.1",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.40",
    "mongoose": "6.9.1",
    "nocache": "3.0.4",
    "nstarter-apm": "0.3.0",
    "nstarter-cache": "0.2.0",
    "nstarter-config": "0.2.0",
    "nstarter-core": "1.0.0",
    "nstarter-entity": "0.3.0",
    "nstarter-grpc": "0.4.0",
    "nstarter-metrics": "0.3.0",
    "nstarter-mongodb": "0.3.0",
    "nstarter-rabbitmq": "0.6.1",
    "nstarter-redis": "0.2.0",
    "nstarter-utils": "0.1.1",
